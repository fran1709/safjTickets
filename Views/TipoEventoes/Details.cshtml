@model devTicket.Models.TipoEvento

@{
    ViewData["Title"] = "Detalles";
}

<h1>Detalles de Tipo Evento</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            <p>Descripcion</p>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Descripcion)
        </dd>
        <dt class = "col-sm-2">
            <p>Creado el</p>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CreatedAt)
        </dd>
        <dt class = "col-sm-2">
            <p>Creado por</p>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CreatedBy)
        </dd>
        <dt class = "col-sm-2">
            <p>Actualizado el </p>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UpdatedAt)
        </dd>
        <dt class = "col-sm-2">
            <p>Actualizado por</p>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UpdatedBy)
        </dd>
        <dt class = "col-sm-2">
           <p>Activo</p>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Active)
        </dd>
    </dl>
</div>
<div class="d-grid gap-2 d-md-block p-2">
    <a class="btn btn-secondary" asp-action="Index"><i class="fa fa-reply" cla></i></a> |
    <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-primary">Editar</i></a>
</div>
